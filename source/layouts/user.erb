<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    
    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "Thankyouspot" %></title>
    
    <%= stylesheet_link_tag "colorbox", "main" %>
    <%= javascript_include_tag  "vendor/jquery", "vendor/bootstrap", "vendor/jquery.photoset-grid", "vendor/jquery.colorbox", "design" %>
	<%= favicon_tag 'images/favicon.ico' %>
  </head>
  
  <body class="<%= page_classes %>">
	<%= partial "partials/user-navigation" %>
    <%= yield %>
	<%= partial "partials/user-footer" %>
  </body>
</html>